<!--
aggregationType = AVERAGE | SUM | OBSERVATION
timeRollUpType = AVERAGE | SUM | CURRENT
clusterRollUpType = INDIVIDUAL | COLLECTIVE
-->
<!--
Set delta = true for metric to report per-min data
-->
<stats>
    <!-- Items in Queues|<queue-name> - data looked up from /queues?api-version=2017-04-01-->
    <stat url="/$Resources/Queues?api-version=2013-07" alias="Queues" filter-name="nodes" children="entries">
        <naming use-entry-name="true"/>
        <metric attr="ActiveMessageCount" alias="ActiveMessageCount" aggregationType = "AVERAGE" timeRollUpType = "AVERAGE" clusterRollUpType = "INDIVIDUAL"/>
        <metric attr="ScheduledMessageCount" alias="ScheduledMessageCount" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="TransferMessageCount" alias="TransferMessageCount" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="TransferDeadLetterMessageCount" alias="TransferDeadLetterMessageCount" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="MaxSizeInMegabytes" alias="MaxSizeInMegabytes" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="DeadLetterMessageCount" alias="DeadLetterMessageCount" aggregationType = "AVERAGE" timeRollUpType = "AVERAGE" clusterRollUpType = "INDIVIDUAL"/>
        <metric attr="MaxDeliveryCount" alias="MaxDeliveryCount" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="SizeInBytes" alias="SizeInBytes" aggregationType = "AVERAGE" timeRollUpType = "AVERAGE" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="MessageCount" alias="MessageCount" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="AvailabilityStatus" alias="AvailabilityStatus" aggregationType = "AVERAGE" timeRollUpType = "AVERAGE" clusterRollUpType = "COLLECTIVE">
            <convert str="Unknown" value="1"/>
            <convert str="Available" value="2"/>
            <convert str="Limited" value="3"/>
            <convert str="Restoring" value="4"/>
        </metric>
        <metric attr="Status" alias="Status" aggregationType = "AVERAGE" timeRollUpType = "AVERAGE" clusterRollUpType = "COLLECTIVE">
            <convert str="Active" value="1"/>
            <convert str="Disabled" value="2"/>
            <convert str="Restoring" value="3"/>
            <convert str="SendDisabled" value="4"/>
            <convert str="ReceiveDisabled" value="5"/>
        </metric>
    </stat>

    <stat url="/$Resources/Topics?api-version=2013-07" alias="Topics" filter-name="nodes" children="entries">
        <metric attr="ActiveMessageCount" alias="ActiveMessageCount" aggregationType = "AVERAGE" timeRollUpType = "AVERAGE" clusterRollUpType = "INDIVIDUAL"/>
        <metric attr="DeadLetterMessageCount" alias="DeadLetterMessageCount" aggregationType = "AVERAGE" timeRollUpType = "AVERAGE" clusterRollUpType = "INDIVIDUAL"/>
        <metric attr="ScheduledMessageCount" alias="ScheduledMessageCount" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="TransferMessageCount" alias="TransferMessageCount" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="TransferDeadLetterMessageCount" alias="TransferDeadLetterMessageCount" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="MaxSizeInMegabytes" alias="MaxSizeInMegabytes" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="SizeInBytes" alias="SizeInBytes" aggregationType = "AVERAGE" timeRollUpType = "AVERAGE" clusterRollUpType = "COLLECTIVE"/>
        <metric attr="Status" alias="status" aggregationType = "AVERAGE" timeRollUpType = "AVERAGE" clusterRollUpType = "COLLECTIVE">
            <convert str="Active" value="1"/>
            <convert str="Disabled" value="2"/>
            <convert str="Restoring" value="3"/>
            <convert str="SendDisabled" value="4"/>
            <convert str="ReceiveDisabled" value="5"/>
        </metric>
        <metric attr="AvailabilityStatus" alias="AvailabilityStatus" aggregationType = "AVERAGE" timeRollUpType = "AVERAGE" clusterRollUpType = "COLLECTIVE">
            <convert str="Unknown" value="1"/>
            <convert str="Available" value="2"/>
            <convert str="Limited" value="3"/>
            <convert str="Restoring" value="4"/>
        </metric>
    </stat>

</stats>